@model Veiculo
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="Colorlib Templates" />
    <meta name="author" content="Colorlib" />
    <meta name="keywords" content="Colorlib Templates" />

    <title>Editar - Veiculo</title>

    <link href="~/vendor/mdi-font/css/material-design-iconic-font.css" rel="stylesheet" media="all" />
    <link href="~/vendor/font-awesome-4.7/css/font-awesome.css" rel="stylesheet" media="all" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i" rel="stylesheet" />
    <link href="~/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">
    <link href="~/css/cadastrar.css" rel="stylesheet" media="all">

</head>
<body>
    <!--<header>
        <div class="container">
            <nav class="menu">
                <ul>
                    <li></li>
                    <li></li>
                    <li></li>
                    <li></li>
                    <li></li>
                </ul>
            </nav>
        </div>
    </header>-->
    <section>
        <div class="page-wrapper bg-gra-03 p-t-45 p-b-50">
            <div class="wrapper wrapper--w790">
                <div class="card card-5">
                    <div class="card-heading">
                        <h2 class="title">Editar Veículo</h2>
                    </div>
                    <div class="card-body">
                        <form asp-action="Editar" asp-controller="Veiculo" method="post">
                            <div asp-validation-summary="ModelOnly"></div>
                            <div class="form-row">
                                <div class="name">Placa</div>
                                <div class="value">
                                    <div class="input-group">
                                        <input class="input--style-5" type="text" asp-for="placa">
                                        <label asp-for="placa" />
                                        <span asp-validation-for="placa" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="name">Marca</div>
                                <div class="value">
                                    <div class="input-group">
                                        <input class="input--style-5" type="text" asp-for="marca">
                                        <label asp-for="marca" />
                                        <span asp-validation-for="marca" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="name">Modelo</div>
                                <div class="value">
                                    <div class="input-group">
                                        <input class="input--style-5" type="text" asp-for="modelo">
                                        <label asp-for="modelo" />
                                        <span asp-validation-for="modelo" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="name">Cor</div>
                                <div class="value">
                                    <div class="input-group">
                                        <input class="input--style-5" type="text" asp-for="cor">
                                        <label asp-for="cor" />
                                        <span asp-validation-for="cor" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="name">Valor diária</div>
                                <div class="value">
                                    <div class="input-group">
                                        <input class="input--style-5" type="text" asp-for="valorDiaria">
                                        <label asp-for="valorDiaria" />
                                        <span asp-validation-for="valorDiaria" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="name">Tipo</div>
                                <div class="value">
                                    <div class="input-group">
                                        <div class="rs-select2 js-select-simple select--no-search">
                                            <label asp-for="tipo" />
                                            <select asp-for="tipo">
                                                <option disabled="disabled" selected="selected">Escolha a opção</option>
                                                <option value="Carro">Carro</option>
                                                <option value="Moto">Moto</option>
                                                <option value="Caminhao">Caminhão</option>
                                            </select>
                                            <span asp-validation-for="tipo" />
                                            <div class="select-dropdown"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <button class="btn btn--radius-2 btn--red" type="submit" value="Editar">Editar</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/select2/select2.min.js"></script>
    <script src="~/vendor/datepicker/moment.min.js"></script>
    <script src="~/vendor/datepicker/daterangepicker.js"></script>
    <script src="~/js/cadastrar.js"></script>

</body>

</html>


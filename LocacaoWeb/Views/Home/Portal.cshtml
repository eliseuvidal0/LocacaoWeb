@model Reserva
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="Colorlib Templates" />
    <meta name="author" content="Colorlib" />
    <meta name="keywords" content="Colorlib Templates" />

    <title>Portal - Locações</title>

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link href="~/vendor/mdi-font/css/material-design-iconic-font.css" rel="stylesheet" media="all" />
    <link href="~/vendor/font-awesome-4.7/css/font-awesome.css" rel="stylesheet" media="all" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i" rel="stylesheet" />
    <link href="~/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">
    <link href="~/css/cadastrar.css" rel="stylesheet" media="all">

</head>

<body>
    <header>

    </header>

    <section>
        <div class="page-wrapper bg-gra-03 p-t-45 p-b-50">
            <div style="text-align: right; margin: 20px 200px 10px 0">
                <a asp-action="Logout" asp-controller="Gerente" class="btn btn--radius-2 btn--red">Logout</a>
            </div>
            <div class="wrapper wrapper--w960">
                <div class="card card-5">
                    <div class="card-heading">
                        <h2 class="title">Portal do funcionário</h2>
                    </div>
                    <div class="card-body">
                        <div class="card-deck">
                            <div class="col-md-4" style="text-align:center">
                                <div class="card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Index" asp-controller="Veiculo">Listar Veículos</a>
                                </div>
                            </div>
                            <div class="col-md-4" style="text-align:center">
                                <div class="portal card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Index" asp-controller="Cliente">Listar Clientes</a>
                                </div>
                            </div>
                            <div class="col-md-4" style="text-align:center">
                                <div class="portal card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Index" asp-controller="Funcionario">Listar Funcionários</a>
                                </div>
                            </div>
                            <div class="col-md-4" style="text-align:center">
                                <div class="card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Index" asp-controller="Locacao">Listar Locações</a>
                                </div>
                            </div>
                            <div class="col-md-4" style="text-align:center">
                                <div class="card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Devolucao" asp-controller="Locacao">Devolver Veículos</a>
                                </div>
                            </div>
                            <div class="col-md-4" style="text-align:center">
                                <div class="card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Reservados" asp-controller="Reserva">Listar Reservas</a>
                                </div>
                            </div>
                            <div class="col-md-4" style="text-align:center">
                                <div class="card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Cadastrar" asp-controller="Funcionario">Novo Funcionário</a>
                                </div>
                            </div>
                            <div class="col-md-4" style="text-align:center">
                                <div class="card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Cadastrar" asp-controller="Veiculo">Novo Veículo</a>
                                </div>
                            </div>
                            <div class="col-md-4" style="text-align:center">
                                <div class="card-5 btn btn--radius-2 btn--green" style="padding:10px; margin: 5px; border: 2px solid #202020;">
                                    <a asp-action="Cadastrar" asp-controller="Locacao">Locar Veículo</a>
                                </div>
                            </div>

                        </div>

                    </div>

                </div>
            </div>
        </div>
        </div>
    </section>

    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/select2/select2.min.js"></script>
    <script src="~/vendor/datepicker/moment.min.js"></script>
    <script src="~/vendor/datepicker/daterangepicker.js"></script>
    <script src="~/js/cadastrar.js"></script>

</body>
</html>


